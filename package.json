{
  "name": "@sailfin/sailfin",
  "version": "0.0.1",
  "description": "A framework for building Sailfin applications",
  "author": "Michael Curtis <michael@sailfin.io>",
  "readme": "README.md",
  "contributors": [
    {
      "name": "Michael Curtis",
      "email": "michael@sailfin.io",
      "url": "https://github.com/mcereal"
    }
  ],
  "maintainers": [
    {
      "name": "Michael Curtis",
      "email": "michael@sailfin.io",
      "url": "https://github.com/mcereal"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SailfinIO/sailfin/issues"
  },
  "homepage": "https://github.com/SailfinIO/sailfin#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SailfinIO/sailfin.git"
  },
  "keywords": [
    "sailfin",
    "framework",
    "router",
    "oidc",
    "openid",
    "openid connect",
    "ouath2",
    "oauth",
    "auth",
    "authentication",
    "authorization",
    "identity",
    "identity provider",
    "identity management",
    "identity federation",
    "identity token",
    "identity token provider",
    "identity token management",
    "identity token federation",
    "typescript",
    "nodejs",
    "session",
    "session management"
  ],
  "scripts": {
    "build": "npm run clean && tsc -b tsconfig.build.json",
    "test": "jest --detectOpenHandles --forceExit --config jest.config.ts",
    "e2e-test": "jest --detectOpenHandles --forceExit --config jest.e2e.config.ts",
    "test:watch": "jest --watch --config jest.config.ts",
    "test:coverage": "jest --detectOpenHandles --forceExit --config jest.config.ts --coverage",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "prepare": "npm run build",
    "clean": "rimraf dist",
    "docs": "typedoc"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.4.15",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@types/cache-manager": "^4.0.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "nock": "^13.5.6",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.27.5",
    "typescript": "^5.7.2"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "package.json"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "publishConfig": {
    "access": "public"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/mcereal"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  }
}
