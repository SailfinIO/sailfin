// examples/advanced/futures-simple.sfn

fn computeTask1() -> number {
    // Dummy task implementation
    return 1;
}

fn computeTask2() -> number {
    // Dummy task implementation
    return 2;
}

async fn main() -> void {
    let future1 = async { computeTask1() };
    let future2 = async { computeTask2() };
    let results: number[] = await [future1, future2];
    print.info("Future results computed successfully");
    print.info(results[0]);
    print.info(results[1]);
}
