// examples/advanced/generic-structures.sfn

struct List<T> {
    items: T[];

    fn add(self, item: T) -> void {
        self.items.append(item);
    }

    fn get(self, index: number) -> T? {
        if (index < 0 || index >= self.items.length) {
            return null;
        }
        return self.items[index];
    }
}
