// examples/concurrency/parallel.sfn

fn computeTask1() -> number {
    return 21;
}

fn computeTask2() -> number {
    return 21;
}

fn main() {
    let results = parallel [
        fn() -> number { return computeTask1(); },
        fn() -> number { return computeTask2(); },
    ];

    print.info("Results: {{results}}");
}
