// Minimal compiler test to isolate import issues

import "lexer.sfn" as Lexer;

fn main() -> void {
    print("Testing minimal import...");
    let source: string = "let x: number = 42;";
    let tokens: Token[] = Lexer.lex(source);
    print("Lexed successfully!");
    print("Token count: " + tokens.length.toString());
}
