struct Token {
    type: string;
    value: string;
}

struct Parser {
    tokens: Token[];
    pos: number;
}

fn peek(parser: Parser) -> Token {
    let nextPos: number = parser.pos + 1;
    if nextPos < parser.tokens.length {
        return parser.tokens[nextPos];
    }
    return new Token { type: "EOF", value: "" };
}
