// Test full generic structures functionality
struct List<T> {
    items: T[];

    fn add(self, item: T) -> void {
        self.items.append(item);
    }

    fn get(self, index: number) -> T? {
        if index < 0 {
            return null;
        }
        return self.items[index];
    }
}

fn main() -> void {
    let stringList = List<string>{ items: [] };
    stringList.add("hello");
    stringList.add("world");
    
    print("First item: " + stringList.get(0));
    print("Second item: " + stringList.get(1));
}
