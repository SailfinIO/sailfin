{
  "scopeName": "source.sfn",
  "patterns": [
    {
      "name": "keyword.control.sfn",
      "match": "\\b(if|else|for|while|return)\\b"
    },
    {
      "name": "variable.sfn",
      "match": "\\b([a-zA-Z_][a-zA-Z0-9_]*)\\b"
    },
    {
      "name": "string.quoted.double.sfn",
      "begin": "\"",
      "end": "\""
    }
  ],
  "repository": {}
}
